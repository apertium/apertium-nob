	(NOT 0 farlige-part)
	(NOT -1 det-prep-infmerke-adjbe)
	(NOT -2 be-nøyt-det)
	(*-1 %være/bli%
         BARRIER fv OR perf-part OR setn-gr OR nom
         LINK -1 n LINK *-1 ("til") BARRIER ikke-adv-adj LINK -1 ("forbindelse") LINK -1 ("med")
