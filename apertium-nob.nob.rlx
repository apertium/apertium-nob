        (-1C adj OR pp LINK NOT 0 gen LINK 0 &&ADJ_N_AGR)
        (NOT 0 clb)
        (1C adj OR pp LINK NOT 0 gen LINK 0 &&ADJ_N_AGR)
        (2C n LINK NOT 0 gen LINK 0 &&ADJ_N_AGR)
