;

MAP:bransjens-og-den-enkelte-bedrifts-situasjon (@nkon) %og/eller% IF
        (-1C n + gen LINK 0 $$NUMBER + def)
        (NOT 0 clb)
        (1 dem)
        (*2C n + gen BARRIER ikke-adj-det LINK 0 $$NUMBER) # allow single-defnes
;

